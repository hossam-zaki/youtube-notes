

{
    "name": "bear",
    "version": "1.0",
    "manifest_version": 2,
    "key": "",
    "content_scripts": [
      {
        "matches": ["<all_urls>"],
        "js": ["content.js", "js/jquery.min.js"]
      }
    ],
    "web_accessible_resources": [
      "popup.html"
   ],
    "background": {
      "scripts": ["background.js","js/jquery.min.js" ]
    },
    "browser_action": {
        "default_title": "Bear"
      },
      "permissions": ["activeTab", "identity",
        "identity.email", "webRequestBlocking", "webRequest", "contextMenus"]

  }